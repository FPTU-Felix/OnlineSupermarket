    </servlet>
    <servlet>
        <servlet-name>OrderForWarehouse</servlet-name>
        <servlet-class>Controller.OrderForWarehouse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartContactController</servlet-name>
        <servlet-class>Controller.CartContactController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReceiverController</servlet-name>
        <servlet-class>Controller.ReceiverController</servlet-class>
