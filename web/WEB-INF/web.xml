        <session-timeout>30</session-timeout>
    </session-config>
    <servlet-mapping>
        <servlet-name>OrderReviewRestController</servlet-name>
        <url-pattern>/orders/api/v1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderDetailsRestController</servlet-name>
        <url-pattern>/order-detail/api/v1</url-pattern>
    </servlet-mapping>
</web-app>
